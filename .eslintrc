{
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "env": {
      "browser": true
    },
    "rules": {
      "consistent-this": ["warn", "me"]
    },
    "globals": {
      "$": "readonly",
      "_": "readonly",
      "__DEBUG__": "readonly"
    }
  }
  